{
  "compilerOptions": {
	"target": "es5",
	"module": "commonjs",
	"moduleResolution": "classic",
	"noImplicitAny": false,
	"removeComments": true,
	"preserveConstEnums": true,
	"sourceMap": true,
	"declaration": false
  },
  "types": [
	"jasmine"
  ],
  "exclude": [
	"lib",
	"node_modules",
	"bower_components"
  ]
}